{
  "name": "booking-api",
  "version": "0.1.0",
  "description": "Reservation system API for bowling lanes and quiz room with Przelewy24 integration.",
  "main": "dist/main.js",
  "scripts": {
    "dev": "tsx watch src/main.ts",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/main.js",
    "start:prod": "node dist/main.js",
    "serve": "node dist/main.js",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"{src,tests,scripts}/**/*.{ts,tsx,js,json}\"",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:unit": "vitest run tests/unit",
    "test:api": "vitest run tests/api",
    "test:e2e": "playwright test",
    "typeorm:migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/database/data-source.ts",
    "typeorm:migration:run": "typeorm-ts-node-commonjs migration:run -d src/database/data-source.ts",
    "typeorm:migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/database/data-source.ts",
    "seed": "tsx src/seeds/001_initial_data.ts",
    "seed:dev": "cross-env NODE_ENV=development tsx src/seeds/001_initial_data.ts",
    "seed:prod": "cross-env NODE_ENV=production tsx src/seeds/001_initial_data.ts",
    "cleanup:holds": "tsx scripts/cleanup-holds.ts",
    "cleanup:pending": "tsx scripts/cleanup-pending-payment.ts",
    "cleanup:all": "tsx scripts/cleanup-all.ts",
    "cleanup:sessions": "tsx scripts/cleanup-all.ts sessions",
    "debug:connections": "tsx scripts/debug-connections.ts",
    "perf:monitor": "tsx scripts/performance-monitor.ts",
    "perf:test": "tsx scripts/performance-test.ts"
  },
  "keywords": [
    "nestjs",
    "typeorm",
    "reservations",
    "przelewy24",
    "booking"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nestjs/common": "^11.1.7",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.7",
    "@nestjs/platform-express": "^11.1.7",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "@paralleldrive/cuid2": "^3.1.0",
    "argon2": "^0.31.2",
    "axios": "^1.6.8",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "helmet": "^7.1.0",
    "joi": "^18.0.1",
    "mysql2": "^3.15.3",
    "nodemailer": "^6.9.8",
    "pino": "^8.19.0",
    "pino-http": "^9.0.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "sqlite3": "^5.1.7",
    "typeorm": "^0.3.27",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.10",
    "@nestjs/testing": "^11.1.7",
    "@playwright/test": "^1.43.1",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^4.17.21",
    "@types/node": "^20.19.23",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "pino-pretty": "^10.3.1",
    "playwright": "^1.43.1",
    "pm2": "^5.3.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.3",
    "typescript": "^5.9.3",
    "vitest": "^1.5.0"
  },
  "engines": {
    "node": ">=18.17.0"
  }
}
